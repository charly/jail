    <div style="text-align: center;">
      <%= link_to "Jail's list", "/jail/" %>
      |
      <%= link_to "Cdnjs's List", "/jail/cdnjs" %>
    </div>
    
  </div>

  <div id="page">
    <%= yield %>
  </div>

</body>
</html>
